fun greet(name: string): string{
  return "Hello " + name;
}

fun bar(f: anon(int,int)->int, o: int) : int{
  return f(o,o);
}

# composition de fonctions
fun bazzuka(f:anon(int)->int,g:anon(int)->int, a:int) :int{
  return f(g(a));
}



